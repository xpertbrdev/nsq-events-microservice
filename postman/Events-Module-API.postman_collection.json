{
  "info": {
    "name": "Events Module API",
    "description": "API para gerenciamento de eventos com sessões transacionais e NSQ",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json",
    "_exporter_id": "1"
  },
  "item": [
    {
      "name": "Sessions",
      "item": [
        {
          "name": "Start Session",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "// Salvar sessionId no environment",
                  "if (pm.response.code === 201) {",
                  "    const response = pm.response.json();",
                  "    pm.environment.set('sessionId', response.sessionId);",
                  "    console.log('Session ID saved:', response.sessionId);",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"filialId\": \"001\",\n  \"filialCNPJ\": \"12345678000190\",\n  \"ambiente\": \"production\",\n  \"sender\": \"erp\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/events/sessions/start",
              "host": ["{{baseUrl}}"],
              "path": ["events", "sessions", "start"]
            },
            "description": "Inicia uma nova sessão de eventos. O sessionId retornado é automaticamente salvo no environment."
          },
          "response": []
        },
        {
          "name": "Add Event",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"data\": {\n    \"produto\": \"Notebook Dell\",\n    \"quantidade\": 5,\n    \"preco\": 3500.00,\n    \"categoria\": \"Informática\"\n  },\n  \"method\": \"create\",\n  \"className\": \"Produto\",\n  \"unico\": \"PROD-001\",\n  \"filialId\": 1,\n  \"filialCnpj\": \"12345678000190\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/events/sessions/{{sessionId}}/events",
              "host": ["{{baseUrl}}"],
              "path": ["events", "sessions", "{{sessionId}}", "events"]
            },
            "description": "Adiciona um evento à sessão ativa. Usa o sessionId do environment."
          },
          "response": []
        },
        {
          "name": "Commit Session",
          "request": {
            "method": "POST",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/events/sessions/{{sessionId}}/commit",
              "host": ["{{baseUrl}}"],
              "path": ["events", "sessions", "{{sessionId}}", "commit"]
            },
            "description": "Finaliza a sessão e publica todos os eventos no NSQ. Usa o sessionId do environment."
          },
          "response": []
        },
        {
          "name": "Rollback Session",
          "request": {
            "method": "POST",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/events/sessions/{{sessionId}}/rollback",
              "host": ["{{baseUrl}}"],
              "path": ["events", "sessions", "{{sessionId}}", "rollback"]
            },
            "description": "Cancela a sessão e descarta todos os eventos. Usa o sessionId do environment."
          },
          "response": []
        },
        {
          "name": "Get Session Status",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/events/sessions/{{sessionId}}/status",
              "host": ["{{baseUrl}}"],
              "path": ["events", "sessions", "{{sessionId}}", "status"]
            },
            "description": "Obtém o status da sessão. Usa o sessionId do environment."
          },
          "response": []
        },
        {
          "name": "Get Session Events",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/events/sessions/{{sessionId}}/events",
              "host": ["{{baseUrl}}"],
              "path": ["events", "sessions", "{{sessionId}}", "events"]
            },
            "description": "Lista todos os eventos da sessão. Usa o sessionId do environment."
          },
          "response": []
        }
      ],
      "description": "Endpoints para gerenciamento de sessões e eventos"
